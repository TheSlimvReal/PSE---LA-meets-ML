{
  "collect": {
    "path": "modules/shared/data/UnlabeledMatrices/",
    "size": 128,
    "amount": 100
  },
  "label": {
    "saving_path": "modules/shared/data/LabeledMatrices/",
    "name": "labeled_matrices",
    "path": "modules/shared/data/UnlabeledMatrices/unlabeled_matrices.hdf5"
  },
  "train": {
    "name": "trained_net",
	"path": "modules/shared/data/LabeledMatrices/",
    "saving_path": "modules/shared/data/TrainedNetwork",
    "train_split": 0.8,


    "num_conv_layers": 1,
    "num_dense_layers": 3,
    "layer_activation": "relu",
    "final_activation": "softmax",
    "dropout": 0.1,

    "batch_size": 25,
    "learning_rate": 0.001,
    "loss": "categorical_crossentropy",
    "epochs": 5

  },
  "classify": {
	"network_path": "modules/shared/data/NeuralNetwork/"
  }
}
